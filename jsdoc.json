{
    "tags": {
        "allowUnknownTags": "all"
    },
    "source": {
        "includePattern": "\\.(jsx|js|ts|tsx)$"
    },
    "plugins": [
        "plugins/markdown",
        "category",
        "cssvar"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./"
    },
    "templates": {
        "logo": {
            "url": "https://avatars1.githubusercontent.com/u/783327?s=200&v=4",
            "width": "1000px",
            "height": "100px",
            "link": "https://flow-ux.aspectron.com/"
        }
    }
}
